{
  "name": "@recap.dev/client",
  "version": "1.0.0",
  "description": "Client library for recap.dev, an open-source cloud observability and tracing solution for modern JavaScript backends.",
  "repository": {
    "type": "git",
    "url": "https://github.com/infinite-cat/recap.dev-client.git"
  },
  "homepage": "https://recap.dev/",
  "main": "dist/client.umd.js",
  "module": "dist/client.es5.js",
  "typings": "dist/types/client.d.ts",
  "author": "Arseny Yankovsky",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "tsc": "tsc --noEmit",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "format": "prettier --write ./src",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "json-stringify-safe": "^5.0.1",
    "lodash-es": "^4.17.21",
    "node-fetch": "^3.1.1",
    "node-sql-parser": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "require-in-the-middle": "^5.1.0",
    "serialize-error": "^6.0.0",
    "shimmer": "^1.2.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@elastic/elasticsearch": "^7.4.0",
    "@types/aws-lambda": "^8.10.48",
    "@types/jest": "^23.3.2",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/lodash-es": "^4.17.3",
    "@types/mongodb": "^3.6.3",
    "@types/mysql": "^2.15.9",
    "@types/node": "^10.11.0",
    "@types/node-fetch": "^2.5.6",
    "@types/serialize-error": "^4.0.1",
    "@types/shimmer": "^1.0.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "aws-lambda": "^1.0.5",
    "colors": "^1.3.2",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.19.0",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "jest-config": "^23.6.0",
    "lodash.camelcase": "^4.3.0",
    "mongodb": "^3.6.3",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "pg": "^8.0.3",
    "prettier": "^2.2.1",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^2.39.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-uglify": "^6.0.4",
    "semantic-release": "^17.3.9",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^23.10.2",
    "ts-node": "^7.0.1",
    "tslib": "^2.1.0",
    "typedoc": "^0.17.4",
    "typescript": "^4.1.5"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "keywords": [
    "tracing",
    "monitoring",
    "server monitoring",
    "observability",
    "dashboard"
  ],
  "publishConfig": {
    "access": "public"
  }
}
